# .env.example â€” copy to .env and edit values
export PROJECT_ID="$(gcloud config get-value core/project)"
export REGION="$(gcloud config get-value run/region || echo northamerica-northeast1)"
env PROJECT_ID="$PROJECT_ID" REGION="$REGION" bash scripts/fill_env.sh --yes
source .env

# Required for FHIR wiring (fill these)
export DATASET_ID=<your_dataset_id>
export STORE_ID=<your_fhir_store_id>

# Bridge Cloud Run (fill these if you work on FHIR Store only)
export BRIDGE_SERVICE=<your_bridge_service_name>
export BRIDGE_URL=<your_bridge_https_url>
# Service account used for Pub/Sub push auth (created automatically with same name if you used the previous step)
# if you work on FHIR Store only
export PUSH_SA=bridge-push@${PROJECT_ID}.iam.gserviceaccount.com
